[loggers]
keys=root,scraping

[handlers]
keys=consoleHandler

[formatters]
keys=simpleFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_scraping]
level=INFO
handlers=consoleHandler
qualname=scraping
propagate=0

[handler_consoleHandler]
class=FileHandler
level=INFO
formatter=simpleFormatter
args=('log/logs.log','a', 'utf-8')

[formatter_simpleFormatter]
format=%(asctime)s - %(levelname)s - %(message)s